{
  "name": "react-mentions-continued",
  "version": "5.0.1",
  "description": "React mentions input (forked from react-mentions)",
  "main": "dist/react-mentions-continued.cjs.js",
  "module": "dist/react-mentions-continued.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "preconstruct build",
    "format": "prettier --write \"{src,jest,demo/src}/**/*.js\"",
    "lint": "eslint --max-warnings=0 --ext .js src jest demo",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=development webpack-dev-server --config demo/webpack.config.js",
    "test": "jest",
    "now-build": "webpack --config demo/webpack.config.js",
    "prepublishOnly": "yarn build",
    "prepare": "husky install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakehedman/react-mentions-continued.git"
  },
  "keywords": [
    "react",
    "mentions",
    "react-component"
  ],
  "browserslist": [
    "chrome >= 50",
    "firefox >= 52",
    "safari >= 10",
    "ie >= 11"
  ],
  "author": "Jake Hedman",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jakehedman/react-mentions-continued/issues"
  },
  "homepage": "https://github.com/jakehedman/react-mentions-continued",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^8.0.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.1",
    "css-loader": "^3.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": ">=6",
    "jasmine-reporters": "^2.3.2",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.0.2",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": ">=10",
    "preconstruct": "^0.0.82",
    "prettier": "^1.10.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "recompose": "^0.30.0",
    "regenerator-runtime": "^0.13.2",
    "style-loader": "^0.23.1",
    "substyle-jss": "^4.0.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "invariant": "^2.2.4",
    "prop-types": "^15.5.8",
    "substyle": "^9.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.3",
    "react-dom": ">=16.8.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
